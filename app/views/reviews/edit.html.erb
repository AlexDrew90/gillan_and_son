<div class="content-manage-wrapper">
  <h1>Update testimonial:</h1>
<%= form_with model: @review do |f| %>
  <%= f.label :description %>
  <br>
  <%= f.text_area :description, size: "60x10" %>
  <br>
  <%= f.label :name %>
  <br>
  <%= f.text_field :name %>
  <br>
  <br>
  <%= f.submit %>
<% end %>
<br>
<%= link_to "Back to admin homepage", cms_path %>
</div>
